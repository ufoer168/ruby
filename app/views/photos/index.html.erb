<table class="table table-striped table-condensed">
  <% @photo.each do |f| %>
    <tr>
      <td><%= f.title %></td>
      <td><%= link_to "Show", photo_path(f) %></td>
      <td><%= link_to "Edit", edit_photo_path(f) %></td>
      <td><%= link_to "Destroy", photo_path(f), method: :delete, data:{confirm: "Are you sure?"} %></td>
    </tr>
  <% end %>
</table>
<%= link_to "New photo", new_photo_path %>
